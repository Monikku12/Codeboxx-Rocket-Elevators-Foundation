<p id="notice"><%= notice %></p>

<h1>Dim Customers</h1>

<table>
  <thead>
    <tr>
      <th>Creation date</th>
      <th>Company name</th>
      <th>Full name</th>
      <th>Company contact email</th>
      <th>Nb elevator</th>
      <th>Customer city</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dim_customers.each do |dim_customer| %>
      <tr>
        <td><%= dim_customer.creation_date %></td>
        <td><%= dim_customer.company_name %></td>
        <td><%= dim_customer.full_name %></td>
        <td><%= dim_customer.company_contact_email %></td>
        <td><%= dim_customer.Nb_Elevator %></td>
        <td><%= dim_customer.customer_city %></td>
        <td><%= link_to 'Show', dim_customer %></td>
        <td><%= link_to 'Edit', edit_dim_customer_path(dim_customer) %></td>
        <td><%= link_to 'Destroy', dim_customer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dim Customer', new_dim_customer_path %>
